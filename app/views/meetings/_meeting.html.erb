<div id="<%= dom_id meeting %>">
  <p>
    <strong>Team:</strong>
    <%= meeting.team_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= meeting.title %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= meeting.date %>
  </p>

  <p>
    <strong>Mode:</strong>
    <%= meeting.mode %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= meeting.url %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= meeting.location %>
  </p>

  <p>
    <strong>Latitude:</strong>
    <%= meeting.latitude %>
  </p>

  <p>
    <strong>Longitude:</strong>
    <%= meeting.longitude %>
  </p>

  <p>
    <strong>Deleted at:</strong>
    <%= meeting.deleted_at %>
  </p>

</div>
