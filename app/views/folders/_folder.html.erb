<div id="<%= dom_id folder %>">
  <p>
    <strong>Name:</strong>
    <%= folder.name %>
  </p>

  <p>
    <strong>Size:</strong>
    <%= folder.size %>
  </p>

  <p>
    <strong>Team:</strong>
    <%= folder.team_id %>
  </p>

  <p>
    <strong>Parent folder:</strong>
    <%= folder.parent_folder_id %>
  </p>

  <p>
    <strong>Deleted at:</strong>
    <%= folder.deleted_at %>
  </p>

</div>
